<h1>Listing feedings</h1>

<table>
  <thead>
    <tr>
      <th>Beehive</th>
      <th>Name</th>
      <th>Food type</th>
      <th>Quantity</th>
      <th>Quantity unit</th>
      <th>Supplements</th>
      <th>Date</th>
      <th>Notes</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedings.each do |feeding| %>
      <tr>
        <td><%= feeding.beehive %></td>
        <td><%= feeding.name %></td>
        <td><%= feeding.food_type %></td>
        <td><%= feeding.quantity %></td>
        <td><%= feeding.quantity_unit %></td>
        <td><%= feeding.supplements %></td>
        <td><%= feeding.date %></td>
        <td><%= feeding.notes %></td>
        <td><%= feeding.value %></td>
        <td><%= link_to 'Show', feeding %></td>
        <td><%= link_to 'Edit', edit_feeding_path(feeding) %></td>
        <td><%= link_to 'Destroy', feeding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feeding', new_feeding_path %>
