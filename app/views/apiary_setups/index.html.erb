<h1>Listing apiary_setups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @apiary_setups.each do |apiary_setup| %>
      <tr>
        <td><%= link_to 'Show', apiary_setup %></td>
        <td><%= link_to 'Edit', edit_apiary_setup_path(apiary_setup) %></td>
        <td><%= link_to 'Destroy', apiary_setup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Apiary setup', new_apiary_setup_path %>
