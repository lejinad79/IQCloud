<%- model_class = ApiaryParametersSetup -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:apiary_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @apiary_parameters_setups.each do |apiary_parameters_setup| %>
      <tr>
        <td><%= link_to apiary_parameters_setup.id, apiary_parameters_setup_path(apiary_parameters_setup) %></td>
        <td><%= apiary_parameters_setup.apiary_id %></td>
        <td><%=l apiary_parameters_setup.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_apiary_parameters_setup_path(apiary_parameters_setup), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      apiary_parameters_setup_path(apiary_parameters_setup),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_apiary_parameters_setup_path,
            :class => 'btn btn-primary' %>
